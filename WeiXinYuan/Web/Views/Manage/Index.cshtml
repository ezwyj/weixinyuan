@{
    ViewBag.Title = "首页";
}

@section styles {
    <link href="@Url.Content("~/assets/plugins/datepicker/bootstrap-datepicker.min.css")" rel="stylesheet" />
}
<div class="container-fluid page-body">
    <div class="panel panel-danger">
        <div class="panel-heading">
            <h5>查询</h5>
            <div class="panel-search form-inline">
                <div class="input-group" id="search-widget">
                    <div class="input-group-btn dropdown">

                        <span class="dropdown-select">标题</span>



                    </div>
                    <input type="text" class="form-control" placeholder="搜索">
                </div>
            </div>
            <div class="panel-option">
                <button class="btn btn-sm btn-primary" id="query">查询</button>
                <button class="btn btn-sm btn-primary" id="add">增加</button>
            </div>
        </div>
        <div class="panel-body">
            <div class="query-result" id="result-list">
            </div>
        </div>
    </div>
</div>
@section scripts {
    <script>
        OP_CONFIG.module = 'home';
        OP_CONFIG.page = 'index';
    </script>
}